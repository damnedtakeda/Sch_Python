   "source": [
    "def fibonacci(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    result = fibonacci(n - 1) + fibonacci(n - 2)\n",
    "    return result\n",
    "\n",
    "n = int(input(\"Введите номер числа Фибоначчи: \"))\n",
    "answer = fibonacci(n)\n",
    "print(f'{n}-е число Фибоначчи: {answer}')"
   ]
  }
