print("Хочешь сняться в кино? [Да/Нет]")
a = input().lower()
if a == "да":
    print("Халк или Локи? [Халк/Локи]")
    b = input().lower()
    if b == 'халк':
        print("Бицуха сколько, брат? [Число]")
        c = int(input())
        if c < 60:
            print('Иди домой, креветка')
        elif c >= 60:
            print("Welcome to the club buddy")
        else:
            print('уци пиши нормально')
    elif b == 'локи':
        print('Кого ты больше любишь? [Маму/Папу]')
        d = input().lower()
        if d == 'папу':
            print('Иди домой к папочке')
        elif d == 'маму':
            print('Иди к папе и спроси кого больше любишь')
        else:
            print('уци пиши нормально')
    else:
        print('уци, пиши нормально')
elif a == 'нет':
    print("Встал вышел")
else:
    print('уци, пиши нормально')
